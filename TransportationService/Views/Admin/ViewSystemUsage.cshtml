@model TransportationService.Models.CustomTable
@{
    Layout = null;
}

<table id="system-usage-table" class="good-style">
    <thead>
        <tr style="border-bottom: 1px solid black; padding: 5px;">
            @foreach (var header in Model.Headers)
            {
                <th style="font-weight: bold; padding: 7px 0px;">@header<i class=" no-opacity"></i></th>
            }
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Model.Rows)
        {
            <tr id="view-tr-@row.ObjectId" data-id="@row.ObjectId">
                @foreach (var col in row.Columns)
                {
                    <td>@col</td>
                }
            </tr>
        }
    </tbody>
</table>
